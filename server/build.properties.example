mqtt.admin.username=atom
mqtt.admin.password=12345678
mqtt.client.tag=ATOM-MDM


# MQTT authentication for more security (supported by Base MDM launcher v5.05 and above)
mqtt.auth=true
mqtt.ssl=true



# Delaying MQTT messages in milliseconds to avoid congestion
mqtt.message.delay=1000
mqtt.message.qos=2

# Fast device search by last characters, here's the length
device.fast.search.chars=5

